{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@screens": [
        "src/screens"
      ],
      "@components": [
        "src/components"
      ],
      "@configs/*": [
        "src/configs/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@assets/*": [
        "assets/*"
      ],
    }
  },
}